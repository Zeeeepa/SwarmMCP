{
  "name": "voltagent",
  "version": "0.1.0",
  "description": "Unified TypeScript agent framework combining serv and SwarmMCP",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./node/tools": {
      "types": "./dist/esm/src/tools/index.d.ts",
      "import": "./dist/esm/src/tools/index.js",
      "require": "./dist/cjs/src/tools/index.cjs"
    },
    "./node/providers": {
      "types": "./dist/esm/src/providers/index.d.ts",
      "import": "./dist/esm/src/providers/index.js",
      "require": "./dist/cjs/src/providers/index.cjs"
    }
  },
  "files": [
    "dist",
    "docker",
    "scripts"
  ],
  "scripts": {
    "build:esm": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig.cjs.json && node scripts/postbuild-cjs.js",
    "build": "npm run build:esm && npm run build:cjs",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --config jest.e2e.config.js",
    "test:performance": "jest --config jest.performance.config.js",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "mcp-server": "node dist/esm/mcp-server/server.js"
  },
  "keywords": [
    "ai",
    "agent",
    "llm",
    "mcp",
    "typescript"
  ],
  "author": "Zeeeepa",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.21.2",
    "fastmcp": "^1.20.5",
    "glob": "^11.0.1",
    "helmet": "^8.1.0",
    "lru-cache": "^10.2.0",
    "openai": "^4.89.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.15.11",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.9.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

